<template id="group" xmlns="">
	<group id="personalName" title="Personal Name">
		<description>The person's name.</description>
		
	</group>
	<group id="birth" title="Birth">
		<description>This context describes a human birth.</description>
		<graph>
			<node id="date"/>
			<node id="birth">
				<link>date</link>
				
				<link>mother</link>
				<link>child</link>
				
			</node>
			<node id="mother"><link>child</link>
			<link>locationMother</link></node>
			<node id="child">
				<link>locationChild</link>
			</node>
			<node id="locationMother"></node>
			<node id="locationChild"></node>
			

			
		</graph>
		
		<field id="dateInstant" type="date" title="Date of Instantaneous Event" placeHolder="Enter date for instantaneous event"/>
		<field id="dateInstantCertainty" type="select" title="Date - Certainty Level" placeHolder="Enter certainty level for date for instantaneous event">
			<option>Uncertain</option>
			<option>Certain</option>
			
		</field>
		
		<field id="dateStart" type="date" title="Start Date" placeHolder="Enter start date for event with time interval"/>
			
		
		<field id="dateStartCertainty" type="select" title="Start Date - Certainty Level" placeHolder="Enter certainty level for start date for event with time interval">
			<option>Uncertain</option>
			<option>Certain</option>
			
		</field>
		
		<field id="dateEnd" type="date" title="End Date" placeHolder="Enter end date for event with time interval">
			
		</field>
		<field id="dateEndCertainty" type="select" title="End Date - Certainty Level" placeHolder="Enter certainty level for end date for event with time interval">
			<option>Uncertain</option>
			<option>Certain</option>
			
		</field>
		
		<field id="dateBefore" type="date" title="Date Before Event" placeHolder="Enter date known to be before event"></field>
		<field id="dateBeforeCertainty" type="select" title="Date Before Event Certainty Level" placeHolder="Enter certainty level for date known to be before event">
			<option>Uncertain</option>
			<option>Certain</option>
			
		</field>
		
		<field id="dateAfter" type="date" title="Date After Event" placeHolder="Enter date known to be after event"/>
		<field id="dateAfterCertainty" type="select" title="Date After Event Certainty Level" placeHolder="Enter certainty level for date known to be after event">
			<option>Uncertain</option>
			<option>Certain</option>
			
		</field>

		
		<field id="child" type="person" title="Child"/>
		<field id="locationChild" type="location" title="Location of Child" placeHolder="Enter location of child"/>
		<field id="mother" type="person" title="Mother"/>
		<field id="locationMother" type="location" title="Location of Mother" placeHolder="Enter location of mother"/>
	</group>
	<group id="death" title="Death">
		<description>This context describes the death of a person.</description>
		
		<graph>
		<node id="date"/>
		<node id="death">
			<link>date</link>
			<link>deceased</link>
			<link>causeOfDeath</link>
		</node>
		<node id="deceased">
			<link>location</link>
			<link>causeOfDeath</link>
		</node>
		
		<node id="location"/>
		
		<node id="causeOfDeath"/>
		
		
		</graph>
		
		<field id="date" type="date" title="Date"/>
		<field id="location" type="location" title="Location"/>
		<field id="causeOfDeath" type="" title="Cause of Death"/>
		<field id="deceased" type="" title="Person Deceased"/>
	</group>
	<group id="familyRelationships" title="Family Relationships">
	<description>This context describes relationships of a person to family members.</description>
	</group>
	
		<group id="UniversityMatriculation" title="University Matriculation">
			<description>This context is concerned with the formal process of entering a university or of becoming eligible to enter by fulfilling certain academic requirements.</description>
			
			
			<graph>
				
				<node id="universityMatriculation">
				<link>date</link>
					<link>matriculant</link>
					<link>matriculationRegister</link>
					<link>personNameEntry</link>
				</node>
				<node id="date"/>
				<node id="matriculant">
					<link>matriculantLocation</link>
					<link>personNameEntry</link>
					<link>matriculationRegister</link>
				</node>
				<node id="matriculantLocation"></node>
				
				<node id="matriculationRegister">
					<link>personNameEntry</link>
				</node>
				
				<node id="personNameEntry"/>
				
				
			</graph>
			
			
			
			<field id="date" type="date" title="Date" placeHolder="Enter date"/>
			<field id="location" type="location" title="Location" placeHolder="Enter location"/>
			<field id="matriculant" type="person" title="Matriculant" placeHolder=""/>
			
			<field id="matriculationRegister" title="MatriculationRegister" placeHolder="Enter matriculation register"/>
			<field id="personNameEntry" title="Personal Name as Entered" placeHolder="Enter the personal name as entered in the matriculation register"/>
			
			
			
		</group>
	<group id="InclusionClassList" title="Inclusion in Class List">
