	<group id="birth" title="Birth">
		<description>Description of a human birth.</description>
	


		<graph>
			<node id="date"/>
			<node id="location"/>
			<node id="birth">
				
				<link rel="has date">date</link>
				<link rel="has location">location</link>
				<link rel="associated with">mother</link>
				<link rel="associated with">child</link>
				<link rel="associated with">father</link>
				
			</node>
			<node id="mother">
				<link rel="mother of">child</link>
				
			</node>
			<node id="child">
				<link rel="has mother">mother</link>
				<link rel="has father">father</link>
			</node>
			
			<node id="father">
				<link rel="father of" objp="fatherOf">child</link>
			</node>
			
			
			
		</graph>


		<field id="date" type="date" title="Date " placeHolder="Enter date "/>
		<field id="location" type="location" title="Location" placeHolder="Enter location"/>

		<field id="child" type="person" title="Child"/>
		<field id="mother" type="person" title="Mother"/>
		<field id="father" type="person" title="Father"/>
	</group>
