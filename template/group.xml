	<group id="UniversityMatriculation" title="University Matriculation">
		<description>This context is concerned with the formal process of entering a university or
			of becoming eligible to enter by fulfilling certain academic requirements.</description>


		<graph>

			<node id="universityMatriculation">
				<link rel="has date">date</link>
				<link rel="associated with">matriculant</link>
				<link rel="used">matriculationRegister</link>
				<link rel="generated">Entry</link>
			</node>
			<node id="date"/>
			<node id="matriculant">
				<link rel="has location">location</link>
				<link rel="has name">Name</link>
				<link rel="has entry in">matriculationRegister</link>
			</node>
			<node id="location"/>

			<node id="matriculationRegister">
				<link rel="has entry">Entry</link>
			</node>

			<node id="Entry">
				<link rel="is entry for">matriculant</link>
				
			</node>
			
			<node id="Name"/>

		</graph>



		<field id="date" type="date" title="Date" placeHolder="Enter date"/>
		<field id="location" type="location" title="Location" placeHolder="Enter location"/>
		<field id="matriculant" type="person" title="Matriculant" placeHolder=""/>

		<field id="matriculationRegister" title="MatriculationRegister"
			placeHolder="Enter matriculation register"/>
		<field id="Entry" title="Personal Name as Entered"
			placeHolder="Enter the personal name as entered in the matriculation register"/>



	</group>
